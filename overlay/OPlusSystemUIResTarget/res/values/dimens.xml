<resources>

    <!--  Allow CornerHandleView and PathSpecCornerPathRenderer to decouple from corner-radius -->
    <dimen name="config_rounded_mask_size">8dp</dimen>

    <!-- Microns/ums (1000 um = 1mm) per pixel for the given device. If unspecified, UI that
         relies on this value will not be sized correctly. -->
    <item name="pixel_pitch" format="float" type="dimen">63.22</item>

    <!-- Location on the screen of the center of the physical power button. -->
    <dimen name="physical_power_button_center_screen_location_y">990px</dimen>

    <!-- The width/height of the unlock icon view on keyguard. -->
    <dimen name="keyguard_indication_margin_bottom">350px</dimen>

</resources>
